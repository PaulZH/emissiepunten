{
  "name": "emissies-vlaanderen",
  "version": "0.0.1",
  "description": "Emission Explorer for Flanders (dev tools)",
  "scripts": {
    "npm-fresh": "rm -rf ./node_modules && npm install",
    "bower-fresh": "rm -rf ./bower_components && bower install",
    "bower-fresh-o": "rm -rf ./bower_components && bower install -o",
    "app": "./node_modules/.bin/polymer serve",
    "test-e2e": "./node_modules/.bin/karma start dev/e2e/config/karma.conf.js",
    "test-js": "./node_modules/.bin/karma start dev/js-specs/config/karma.conf.js",
    "build": "node scripts/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/bnowack/emissies-vlaanderen.git"
  },
  "keywords": [
    "vlaanderen",
    "emissies"
  ],
