    "test-e2e": "./node_modules/.bin/karma start dev/e2e/config/karma.conf.js",
    "test-js": "./node_modules/.bin/karma start dev/js-specs/config/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/bnowack/emissies-vlaanderen.git"
  },
