  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://gitlab.com/bnowack/emissies-vlaanderen/issues"
  },
  "homepage": "https://gitlab.com/bnowack/emissies-vlaanderen",
  "dependencies": {},
  "devDependencies": {
    "bower": "^1.8.4",
    "fs-extra": "^6.0.1",
    "glob": "latest",
    "html-minifier": "^3.5.15",
    "polymer-cli": "^0.12.0",
    "replace-in-file": "^3.4.0",
    "rimraf": "latest"
  }
